@model IEnumerable<PictureStore.Models.Genre>

@{
    ViewBag.Title = "Store";
}

<h2>Photography</h2>
<p>
    Select from @Model.Count() categories:
</p>

<ul>
    @foreach (var genre in Model)
    {
        <li>@Html.ActionLink(genre.Name, "Browse", new {genre = genre.Name}) </li>
        

        
    }
</ul>

